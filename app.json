{
  "name": "MusicChannelManagerV2",
  "env": {
    "DYNAMIC_CAPTION": {
      "description": "(Optional) Set this to TRUE if you want a dynamic caption be added to the musics.",
      "value": "TRUE",
      "required": false
    },
    "CUSTOM_TAG": {
      "description": "(Optional) Add a username to be appended to the music tags.",
      "value": "@my_channel",
      "required": false
    },
    "API_ID": {
      "description": "Add your telegram API_ID",
      "value": ""
    },
    "API_HASH": {
      "description": "Add your telegram API_HASH",
      "value": ""
    },
    "CHANNEL_USERNAME": {
      "description": "Your telegram channel username.",
      "value": "@my_channel"
    },
    "BOT_TOKEN": {
      "description": "Your bot token, as a string.",
      "value": ""
}
  },
  "buildpacks": [
    {
      "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest"
    },
    {
      "url": "heroku/python"
    }
  ]
}
